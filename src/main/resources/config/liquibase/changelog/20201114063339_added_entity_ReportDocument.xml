<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    
    <!--
        Added the entity ReportDocument.
    -->
    <changeSet id="20201114063339-1" author="jhipster">
        <addColumn schemaName= "public"
			tableName="report_document" >
            <column name="status" type="varchar(255)">
                <constraints nullable="true" />
            </column>
        </addColumn>
        <addColumn schemaName= "public"
        tableName="report_document" >
        <column name="created" type="varchar(255)">
            <constraints nullable="true" />
        </column>
        </addColumn>
        <addColumn schemaName= "public"
        tableName="report_document" >
        <column name="modified" type="varchar(255)">
            <constraints nullable="true" />
        </column>
        </addColumn>
        <addColumn schemaName= "public"
        tableName="report_document" >
        <column name="file_name" type="varchar(255)">
            <constraints nullable="true" />
        </column>
        </addColumn>
        <addUniqueConstraint columnNames="name"  
        constraintName="uk_report_document_name"  
        schemaName="public"  
        tableName="report_document"/>  
    </changeSet>
    <!-- jhipster-needle-liquibase-add-changeset - JHipster will add changesets here -->
</databaseChangeLog>
